<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:mockito="http://www.mockito.org/spring/mockito"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans 
		http://www.springframework.org/schema/beans/spring-beans.xsd
    	http://www.mockito.org/spring/mockito 
    	https://bitbucket.org/kubek2k/springockito/raw/tip/springockito/src/main/resources/spring/mockito.xsd">

	<mockito:mock id="cloudManager" class="pl.cyfronet.coin.impl.manager.CloudManager" />
	
	<mockito:mock id="actionFactory" class="pl.cyfronet.coin.impl.action.ActionFactory" />

	<mockito:mock id="authenticationHandler"
		class="pl.cyfronet.coin.impl.security.mi.MasterInterfaceAuthenticationHandler" />

	<mockito:mock id="authenticationInterceptor" class="org.apache.cxf.jaxrs.ext.RequestHandler" />
</beans>