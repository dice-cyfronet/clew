<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
		xmlns:g="urn:import:com.google.gwt.user.client.ui"
		xmlns:b="urn:import:org.gwtbootstrap3.client.ui">
	<ui:with type="pl.cyfronet.coin.clew.client.widgets.initialconfigembed.InitialConfigEmbedMessages" field="messages"/>
	<ui:style type="pl.cyfronet.coin.clew.client.widgets.initialconfigembed.InitialConfigEmbedView.Styles">
		.header {
			margin-top: 10px;
			font-weight: bold;
		}
	</ui:style>
	<b:Modal ui:field="modal" title="{messages.modalTitle}" closable="false" dataKeyboard="false" fade="true" dataBackdrop="STATIC">
		<b:ModalBody>
			<g:HTML><ui:text from="{messages.about}"/></g:HTML>
			<g:FlowPanel ui:field="parameterContainer"/>
		</b:ModalBody>
		<b:ModalFooter>
			<b:Button ui:field="start" icon="PLAY" type="SUCCESS" dataLoadingText="&lt;i class='fa fa-spinner fa-spin'&gt;&lt;/i&gt; {messages.startButtonLabel}"><ui:text from="{messages.startButtonLabel}"/></b:Button>
			<b:Button ui:field="close"><ui:text from="{messages.closeButtonLabel}"/></b:Button>
		</b:ModalFooter>
	</b:Modal>
</ui:UiBinder>