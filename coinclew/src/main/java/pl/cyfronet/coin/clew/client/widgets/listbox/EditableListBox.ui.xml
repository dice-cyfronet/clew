<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
		xmlns:g="urn:import:com.google.gwt.user.client.ui"
		xmlns:b="urn:import:org.gwtbootstrap3.client.ui">
	<ui:style>
	</ui:style>
	<g:HTMLPanel addStyleNames="form-group">
		<g:HTMLPanel addStyleNames="input-group">
			<div class="input-group-btn">
				<button ui:field="dropdownButton" class="btn btn-default dropdown-toggle" data-toggle="dropdown">
					<span class="caret"></span>
				</button>
				<ul ui:field="list" class="dropdown-menu"></ul>
			</div>
			<b:TextBox ui:field="textBox"/>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder>